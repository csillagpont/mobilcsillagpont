<?xml version='1.0' encoding='utf-8'?>
<widget id="hu.reformatus.mobilcsillagpont" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="android-package" value="com.phonegap.plugins.barcodescanner.BarcodeScanner" />
    </feature>
    <allow-intent href="market:*" />
    <preference name="android-targetSdkVersion" value="19" />
    <preference name="android-minSdkVersion" value="10" />
    <name>MobilCsillagPont</name>
    <description>
        Csillagpont mobil application
    </description>
    <author email="jozsef.gyori@gmail.com" href="http://www.javadev.info">
        Csillagpont Csapat
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
